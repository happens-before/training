<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	   default-lazy-init="true">

	<bean id="userMapper" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.qunar.fresh2017.spring.mapper.UserMapper"/>
	</bean>
	<bean id="userService" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="com.qunar.fresh2017.spring.service.UserService"/>
	</bean>
</beans>
